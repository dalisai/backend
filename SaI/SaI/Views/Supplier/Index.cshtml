@model IEnumerable<SaI.Models.Supplier>

@{
    ViewBag.Title = "Index";
}
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">List of Category</h3>
            <div class="box-tools">
                <a class="btn btn-primary" href="/Supplier/Add"> Add Category </a><br /> <br /> &nbsp;
            </div>
        </div>
        <div class="box">
            <div class="box-header">
                <h3 class="box-title"></h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="dataTables_wrapper form-inline dt-bootstrap" id="example1_wrapper">
                    <div class="row">
                        <div class="col-sm-12">
                            <table class="table table-bordered table-striped dataTable" role="grid" id="example1" aria-describedby="example1_info">
                                <thead>
                                    <tr role="row">
                                        <th>@Html.DisplayNameFor(model => model.Description)</th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model) {
                                        <tr>
                                            <td class="sorting_1">
                                                @Html.DisplayFor(modelItem => item.Description)
                                            </td>
                                            <td>
                                                @Html.ActionLink("Edit", "Edit", new { id = item.CategoryID }, new { @class = "btn btn-warning glyphicon glyphicon-pencil" }) &nbsp;&nbsp;
                                                @Html.ActionLink("Delete", "Delete", new { id = item.CategoryID }, new { @class = "btn btn-danger 	glyphicon glyphicon-trash" })
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.box-body -->
        </div>
    </div>
</section>








































